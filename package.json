{
  "name": "vipfy_graphql_server",
  "author": "Gh05d",
  "license": "ISC",
  "version": "1.0.0",
  "description": "A GraphQL server backend for Vipfy",
  "main": "index.js",
  "scripts": {
    "serve": "ENVIRONMENT=production node dist/index.js",
    "build": "rm -rf dist && babel src -d dist --ignore 'tests'",
    "start": "ENVIRONMENT=development nodemon --exec babel-node src/index.js",
    "test": "npm run prepare-test-db && ENVIRONMENT=testing jest --verbose --no-cache --forceExit",
    "play": "babel-node src/helpers/play.js",
    "prepare-test-db": "./prepare_vipfy_test_db.sh",
    "lint": "eslint ."
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@google-cloud/storage": "^1.6.0",
    "apollo-server-express": "^1.3.2",
    "apollo-upload-server": "^5.0.0",
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "graphql": "^0.12.3",
    "graphql-subscriptions": "^0.5.6",
    "graphql-tools": "^1.0.0",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.5",
    "node-mailjet": "^3.2.1",
    "pg": "^7.4.1",
    "pipedrive": "^3.0.2",
    "querystring": "^0.2.0",
    "sequelize": "^4.32.6",
    "soap": "^0.23.0",
    "stripe": "^5.5.0",
    "subscriptions-transport-ws": "^0.9.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.8.0",
    "faker": "^4.1.0",
    "jest": "^22.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gh05d@bitbucket.org/vipfymarketplace/vipfy-backend.git"
  },
  "keywords": [
    "graphql",
    "nodejs",
    "sequelize"
  ],
  "homepage": "https://bitbucket.org/vipfymarketplace/vipfy-backend#readme"
}
