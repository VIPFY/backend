{
  "name": "vipfy_graphql_server",
  "author": "Gh05d",
  "license": "ISC",
  "version": "1.0.0",
  "description": "A GraphQL server backend for Vipfy",
  "main": "index.js",
  "scripts": {
    "serve": "ENVIRONMENT=production node dist/index.js",
    "build": "rm -rf dist && babel src -d dist --ignore 'tests' --copy-files",
    "start": "ENVIRONMENT=development nodemon --exec babel-node src/index.js",
    "test": "npm run prepare-test-db && ENVIRONMENT=testing jest --verbose --no-cache --forceExit",
    "play": "babel-node src/helpers/play.js",
    "prepare-test-db": "./prepare_vipfy_test_db.sh",
    "lint": "eslint .",
    "publish": "npx pm2 deploy ecosystem.config.js dev update",
    "publish-prod": "npx pm2 deploy ecosystem.config.js production update"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@google-cloud/profiler": "^0.1.14",
    "@google-cloud/storage": "^1.6.0",
    "@google-cloud/trace-agent": "^2.10.1",
    "apollo-server-express": "^1.3.2",
    "apollo-upload-server": "^5.0.0",
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "dataloader-sequelize": "^1.7.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "formidable": "^1.2.1",
    "graphql": "^0.12.3",
    "graphql-subscriptions": "^0.5.6",
    "graphql-tools": "^1.0.0",
    "is-stream": "^1.1.0",
    "iterate-object": "^1.3.2",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.1",
    "mustache": "^2.3.0",
    "node-mailjet": "^3.2.1",
    "nodeice": "^3.0.8",
    "noop6": "^1.0.7",
    "one-by-one": "^3.2.6",
    "pg": "^7.4.1",
    "phantom-html-to-pdf": "^0.5.6",
    "phantomjs-prebuilt": "^2.1.16",
    "pipedrive": "^3.0.2",
    "querystring": "^0.2.0",
    "read-utf8": "^1.2.8",
    "same-time": "^2.3.3",
    "sequelize": "^4.32.6",
    "soap": "^0.24.0",
    "stripe": "^5.5.0",
    "subscriptions-transport-ws": "^0.9.5",
    "ul": "^5.2.13"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.8.0",
    "faker": "^4.1.0",
    "jest": "^22.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gh05d@bitbucket.org/vipfymarketplace/vipfy-backend.git"
  },
  "keywords": [
    "graphql",
    "nodejs",
    "sequelize"
  ],
  "homepage": "https://bitbucket.org/vipfymarketplace/vipfy-backend#readme"
}
